

<h1>Airbnb</h1>
<p>Featured Listing</p>

    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            <% @listing.each do |listing| %>
            <tr>
                <td><%= listing.name %></td>
                <td><%= listing.price %></td>
            </tr>
            <% end %>
        </tbody>
    </table>