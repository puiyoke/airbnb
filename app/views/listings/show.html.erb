  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
  <% @listing.images.each do |image| %>
    <div class="carousel-item <%= 'active' if image == @listing.images[0] %>">
      <img class="img-fluid" src= <%=image.url%> class="images">
    </div>
    <% end %>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

Property name: <%=@listing.name%><br>
Price per night: <%=@listing.price%><br>
Property type: <%=@listing.property_type%><br>
Number of guests: <%=@listing.guest_number%><br>
Description: <%=@listing.description%><br>
Country: <%=@listing.country%><br>
Address: <%=@listing.address%>, <%=@listing.zipcode%>, <%=@listing.city%><br>
Verified? <%=@listing.verified%><br>

<% if current_user.super_admin? -%>
    <a href="/listings/<%= @listing.id%>/verify">verify</a><br>
<%end%>

<% if signed_in? -%>
    <a href="/listings/<%= @listing.id%>/reservation">Book/Reserve</a><br>
<%end%>
<% if @listing.user_id == current_user.id -%>
  <a href="/listings/<%= @listing.id%>/edit">Edit Listing</a><br>
<%end%>